Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "This", " ", "converts", " ", "the", " ", "sample", " ", "of", " ", "the", 
    " ", "uniform", " ", "distribution", " ", "to", " ", "a", " ", "sample", 
    " ", "of", " ", "the", " ", "normal", " ", 
    RowBox[{"distribution", ":"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Mu]", "=", "0"}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{
    "Mean", " ", "of", " ", "the", " ", "normal", " ", "distribution"}], 
    "*)"}], "\n", 
   RowBox[{
    RowBox[{"\[Sigma]", "=", "1"}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{
    "Standard", " ", "deviation", " ", "of", " ", "the", " ", "normal", " ", 
     "distribution"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"sample", "=", 
     RowBox[{"{", "}"}]}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{
    "Initialize", " ", "empty", " ", "list", " ", "for", " ", "the", " ", 
     "sample"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"While", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Length", "[", "sample", "]"}], "<", 
      SuperscriptBox["10", "5"]}], ",", 
     RowBox[{"(*", 
      RowBox[{"Draw", " ", "100000", " ", "samples"}], "*)"}], 
     RowBox[{
      RowBox[{"u1", "=", 
       RowBox[{"RandomReal", "[", "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "Generate", " ", "first", " ", "random", " ", "value", " ", "from", 
        " ", "the", " ", "uniform", " ", "distribution"}], "*)"}], 
      RowBox[{"u2", "=", 
       RowBox[{"RandomReal", "[", "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "Generate", " ", "second", " ", "random", " ", "value", " ", "from", 
        " ", "the", " ", "uniform", " ", "distribution"}], "*)"}], 
      RowBox[{"x", "=", 
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{
          RowBox[{"-", "2"}], " ", 
          RowBox[{"Log", "[", "u1", "]"}]}], "]"}], " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", " ", "Pi", " ", "u2"}], "]"}]}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Compute", " ", "the", " ", "Box"}], "-", 
        RowBox[{"Muller", " ", "transform"}]}], "*)"}], 
      RowBox[{"z", "=", 
       RowBox[{"\[Mu]", "+", 
        RowBox[{"\[Sigma]", " ", "x"}]}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "Transform", " ", "to", " ", "a", " ", "sample", " ", "from", " ", 
        "the", " ", "normal", " ", "distribution"}], "*)"}], 
      RowBox[{"AppendTo", "[", 
       RowBox[{"sample", ",", "z"}], "]"}]}]}], " ", 
    RowBox[{"(*", 
     RowBox[{
     "Add", " ", "the", " ", "sample", " ", "to", " ", "the", " ", "list"}], 
     "*)"}], "]"}], "\n", 
   RowBox[{
    RowBox[{"normaldist", "=", "sample"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"Histogram", "[", 
    RowBox[{"normaldist", ",", "Automatic", ",", "\"\<PDF\>\"", ",", 
